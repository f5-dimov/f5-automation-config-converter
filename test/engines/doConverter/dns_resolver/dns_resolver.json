{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "Common": {
        "class": "Tenant",
        "dnsResolver": {
            "class": "DNS_Resolver",
            "answerDefaultZones": true,
            "cacheSize": 5767167,
            "randomizeQueryNameCase": false,
            "routeDomain": "0",
            "forwardZones": [
                {
                    "name": "forward.net",
                    "nameservers": [
                        "10.10.10.10:53",
                        "10.10.10.11:53"
                    ]
                }
            ],
            "useIpv6": false,
            "useUdp": false
        }
    }
}
