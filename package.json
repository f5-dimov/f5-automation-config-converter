{
  "name": "f5-automation-config-converter",
  "version": "1.17.0",
  "author": "F5 Networks",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f5devcentral/f5-automation-config-converter.git"
  },
  "description": "F5 Automation Config Converter",
  "scripts": {
    "test": "NODE_ENV=test mocha test --recursive --timeout 60000 --exit",
    "watch": "NODE_ENV=test mocha test --recursive --watch --timeout 30000 --exit",
    "lint": "eslint .",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "start": "node init.js serve",
    "copyright": "node scripts/dev/write-copyright.js cicd",
    "build": "source scripts/build/image-build.sh"
  },
  "dependencies": {
    "@f5devcentral/f5-teem": "^1.5.0",
    "commander": "^8.3.0",
    "decompress": "^4.2.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "multer": "^1.4.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@f5devcentral/atg-shared-utilities": "^0.4.0",
    "@f5devcentral/eslint-config-f5-atg": "latest",
    "ajv": "6.12.6",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "mocha": "^9.1.3",
    "node-forge": "^0.10.0",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1"
  },
  "eslintConfig": {
    "extends": [
      "@f5devcentral/eslint-config-f5-atg"
    ],
    "parserOptions": {
      "ecmaVersion": 9
    }
  },
  "eslintIgnore": [
    "autotoolDeps",
    "scripts"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text",
      "json-summary"
    ],
    "exclude": [
      "autotoolDeps/**",
      "coverage/**",
      "docs/**",
      "test/**",
      "scripts/**"
    ]
  }
}
